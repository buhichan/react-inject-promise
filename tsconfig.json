{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "importHelpers": true,
      "moduleResolution": "node",
      "noStrictGenericChecks": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "removeComments": false,
      "noImplicitAny": false,
      "jsx":"react",
      "allowJs": true,
      "watch": true,
      "lib":["es6","scripthost","dom","es2016.array.include","es2015.promise"]
    }
  }